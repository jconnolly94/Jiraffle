{
  "name": "jiraffle",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm:server\" \"npm:client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "npm run build --prefix client",
    "prepare": "husky install"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "concurrently": "^8.2.1",
    "husky": "^8.0.3"
  }
}
